{
    "name": "heycommunity/heycommunity-backend",
    "description": "The HeyCommunity backend. See: https://www.heycommunity.com/",
    "keywords": ["HeyCommunity", "heycommunity-backend", "SNS", "Social", "Community"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "arcanedev/log-viewer": "^8.3",
        "encore/laravel-admin": "^1.8",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "geoip2/geoip2": "^2.12",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/agent": "^2.6",
        "jxlwqq/simditor": "^1.0",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.9",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "nwidart/laravel-modules": "8.2.*",
        "overtrue/laravel-wechat": "^6.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "bluemmb/faker-picsum-photos-provider": "^2.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "overtrue/laravel-lang": "~5.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "supgeek-rod/faker-zh": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers/system.php",
            "app/Helpers/utility.php",
            "app/Helpers/util.php",
            "app/Helpers/develop.php",
            "app/Helpers/faker.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
